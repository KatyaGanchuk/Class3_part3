#include <iostream>
#include <ctime>
#include <cstdlib>
using namespace std;

int main() {
 srand(time(NULL)); 
    const int size = 10;
    int A[size];
    for(int i = 0; i < size; i++) {
        A[i] = rand() % 10 + 1; 
    }

    for(int i = 0; i < size-1; i++) {
        if(A[i] < A[size-1]) {
            cout << A[i] << endl;
            
    }

    cout <<0  << endl; 
   
}
}
